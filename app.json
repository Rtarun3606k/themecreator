{
  "expo": {
    "name": "Theme Creator",
    "slug": "themecreator",
    "scheme": "themecreator",
    "version": "2.3.2",
    "orientation": "portrait",
    "icon": "./assets/TC.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rtarun3606k.themecreator"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rtarun3606k.themecreator"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-splash-screen",
        {
          "ios": {
            "backgroundColor": "#000000",
            "image": "./assets/splash-icon.png",
            "resizeMode": "cover"
          },
          "android": {
            "backgroundColor": "#000000",
            "image": "./assets/splash-icon.png",
            "imageWidth": 150
          }
        }
      ]
    ]
  },
  "extra": {
    "router": {},
    "eas": {
      "projectId": "16be0909-9037-4470-a753-14f0e2369063"
    }
  }
}